{% extends '@AimeosShop/base.html.twig' %}

{% block aimeos_header %}
    <title>Basket</title>
    {{ (aiheader['locale/select'] ?? '')|raw }}
    {{ (aiheader['catalog/search'] ?? '')|raw }}
    {{ (aiheader['catalog/tree'] ?? '')|raw }}
    {{ (aiheader['basket/bulk'] ?? '')|raw }}
    {{ (aiheader['basket/standard'] ?? '')|raw }}
    {{ (aiheader['basket/related'] ?? '')|raw }}
{% endblock %}

{% block aimeos_head_nav %}
    {{ (aibody['catalog/tree'] ?? '')|raw }}
{% endblock %}

{% block aimeos_head_locale %}
    {{ (aibody['locale/select'] ?? '')|raw }}
{% endblock %}

{% block aimeos_head_search %}
    {{ (aibody['catalog/search'] ?? '')|raw }}
{% endblock %}

{% block aimeos_body %}
    <div class="container-fluid">
        {{ (aibody['basket/standard'] ?? '')|raw }}
        {{ (aibody['basket/bulk'] ?? '')|raw }}
        {{ (aibody['basket/related'] ?? '')|raw }}
    </div>
{% endblock %}
